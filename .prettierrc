{
   "arrowParens": "avoid",
   "bracketSpacing": true,
   "endOfLine": "lf",
   "bracketSameLine": true,
   "htmlWhitespaceSensitivity": "css",
   "printWidth": 260,
   "quoteProps": "as-needed",
   "semi": true,
   "singleQuote": false,
   "tabWidth": 2,
   "trailingComma": "all",
   "jsxBracketSameLine": true,
   "jsxSingleQuote": false,
   "singleAttributePerLine": false,
   "plugins": ["prettier-plugin-tailwindcss"]
 }